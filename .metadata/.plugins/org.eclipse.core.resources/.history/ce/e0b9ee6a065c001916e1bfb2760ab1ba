package edu.autocar.start;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class CameraController {

	@GetMapping("/monitor")
	public void monitor() {
	}

	@GetMapping("/camera")
	public void camera() {
		System.out.println("컨트롤ㄹ러");
	}
}
